{
  "require": {
    "php": "^8.0",
    "yiisoft/yii2": "~2.0.43",
    "yiisoft/yii2-redis": "^2.0",
    "zhanguangcheng/linkerman": "~0.2.0",
    "yidas/yii2-bower-asset": "2.0.13.1"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.1.0",
    "yiisoft/yii2-gii": "~2.2.0"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/*": true
    }
  },
  "autoload": {
    "psr-4": {
      "app\\": "./"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "yii\\composer\\Installer::postInstall",
      "app\\Installer::fix"
    ]
  },
  "extra": {
    "yii\\composer\\Installer::postInstall": {
      "setPermission": [
        {
          "runtime": "0777",
          "web/assets": "0777",
          "yii": "0755"
        }
      ],
      "generateCookieValidationKey": [
        "config/web.php"
      ]
    }
  }
}
